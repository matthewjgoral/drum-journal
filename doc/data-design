NOTE
{:name "Name" => String? Autogenerated? SD-drum-p-R-f-O HH-closed-_-R-_-x
 :instrument SD/HH/BD/CR/RD/HT/LT/CB/HF
 :articulartion drum/rimshot/rim/cross-stick/edge/bow/bell/choked/open/closed
 :dynamics p/f [no dynamics means neutral]
 :limb R/L/K/H
 :grace-note flam/drag [or nothing]
 :representation O [single character]}
 
CHORD
{:name ???
 :notes [note1 note2]}

PHRASE
{:name "Cold sweat groove question"
 :display-name "GROOVE QUESTION"
 :count 4
 :subdivision 4
 :contents [[chord11 note1e note1&  note1a]
            [chord22 note2e chord2& note2a]
            [...]]
 :remarks "some text"}

SECTION
{:name "Verse"
 :shortname "V1"
 :bars 8
 :phrases [groove1 groove1 groove1 fill]
 :remarks "some text"}

PIECE
{:name  "Cold sweat groove"
 :tempo 120
 :feel  "Quick"
 :song  "Cold sweat"
 :artist "James Brown"
 :drummer "Clyde Stubblefield"
 :album "Some album"
 :year "1972"
 :remarks "some text"
 :sections [vector of sections]}

PERFORMANCES
{:music "piece section or phrase name" (if section display name of piece it is from)
 :date
 :duration (in seconds)
 :tempo
 :remarks}

SESSION
{:number [autoincrements]
 :date
 :start-time
 :end-time
 :plays [vector of performances]
 :remarks}

GIG
{:date
 :venue
 :band
 :setlist
 :time
 :reflection}
